{
    "mcpServers": {
      "sequential-thinking": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-sequential-thinking"
        ]
      },
      "brave-search": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-brave-search"
        ],
        "env": {
          "BRAVE_API_KEY": "YOUR_BRAVE_API_KEY_HERE"
        }
      },
      "snowflake": {
        "command": "uvx",
        "args": [
          "snowflake-labs-mcp",
          "--service-config-file",
          "/Users/<USERNAME>/.snowflake/snowflake-mcp-config.yaml",
          "--connection-name",
          "<CONNECTION_NAME>"
        ]
      }
    }
  }